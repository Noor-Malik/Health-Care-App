<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/home_Fragment"
        android:name="com.example.homerentalapp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home_Fragment_to_setting_Fragment"
            app:destination="@id/setting_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/setting_Fragment"
        android:name="com.example.homerentalapp.setting_Fragment"
        android:label="fragment_setting_"
        tools:layout="@layout/fragment_setting_">
        <action
            android:id="@+id/action_setting_Fragment_to_theme_fragment"
            app:destination="@id/theme_fragment" />
    </fragment>
    <fragment
        android:id="@+id/theme_fragment"
        android:name="com.example.homerentalapp.theme_fragment"
        android:label="fragment_theme_fragment"
        tools:layout="@layout/fragment_theme_fragment" />
</navigation>